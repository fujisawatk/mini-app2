<div class="contents-n">
  <h3 class="contents-title">
    編集する
  </h3>
  <%= form_with model: @tweet, local: true do |form| %>
    <div class="form">
      <div class="input">
        <%= form.label :title %>
        <%= form.text_field :title %>
      </div>
      <div class="input">
        <%= form.label :image %>
        <%= form.text_field :image %>
      </div>
      <div class="text">
      <%= form.label :text %>
      <%= form.text_area :text %>
    </div>
      <div class="submit">
      <%= form.submit "Send" %>  
    </div>
  <% end %>
  </div>
</div>